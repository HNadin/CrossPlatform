@model Lab6.Models.Transaction

<h1>Transaction Details</h1>

<div>
    <h4>Transaction</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Account Number
        </dt>
        <dd class="col-sm-10">
            @Model.AccountNumber
        </dd>
        <dt class="col-sm-2">
            Merchant ID
        </dt>
        <dd class="col-sm-10">
            @Model.MerchantId
        </dd>
        <dt class="col-sm-2">
            Transaction Type Code
        </dt>
        <dd class="col-sm-10">
            @Model.TransactionTypeCode
        </dd>
        <dt class="col-sm-2">
            Transaction Date and Time
        </dt>
        <dd class="col-sm-10">
            @Model.TransactionDateTime
        </dd>
        <dt class="col-sm-2">
            Transaction Amount
        </dt>
        <dd class="col-sm-10">
            @Model.TransactionAmount
        </dd>
        <dt class="col-sm-2">
            Other Details
        </dt>
        <dd class="col-sm-10">
            @Model.OtherDetails
        </dd>
    </dl>
    <a asp-action="Index">Back to List</a>
</div>